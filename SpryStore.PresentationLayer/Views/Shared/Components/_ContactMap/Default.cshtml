@model List<Address>

<div class="find-us">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="section-heading">
                    <h2>HARİTADAKİ KONUMUMUZ</h2>
                </div>
            </div>
            @foreach (var item in Model)
            {
                <div class="col-md-8">
                    <div id="map">
                        <iframe src="@item.MapLocation" width="100%" height="330px" frameborder="0" style="border:0" allowfullscreen></iframe>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="left-content">
                        <h4>@item.AboutTitle</h4>
                        <p>@item.AboutDetails1<br><br>@item.AboutDetails2</p>
                        <ul class="social-icons">
                            <li><a href="#"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#"><i class="fa fa-behance"></i></a></li>
                        </ul>
                    </div>
                </div>
            }
        </div>
    </div>
</div>